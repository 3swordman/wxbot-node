{
    "compilerOptions": {
        "esModuleInterop": true,
        "strict": true,
        "alwaysStrict": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "strictPropertyInitialization": false,
        "target": "ESNext",
        "moduleResolution": "NodeNext",
        "skipLibCheck": true
    },
    "include": ["."],
    "exclude": ["node_modules"]
}